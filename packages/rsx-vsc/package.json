{
  "name": "rsx-vsc",
  "displayName": "rsx-syntax-highlighting",
  "description": "An extension that enables syntax highlighting for the rsx language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rsx",
      "aliases": ["rsx", "rsx"],
      "extensions": [".rsx"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rsx",
      "scopeName": "source.rsx",
      "path": "./syntaxes/rsx.tmLanguage.json"
    }]
  }
}
